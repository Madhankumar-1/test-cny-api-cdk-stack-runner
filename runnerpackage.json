{
     "name": "deployment",
     "version": "0.1.0",
     "bin": {
          "deployment": "bin/deployment.js"
     },
     "scripts": {
          "start": "sh ./scripts/start.sh",
          "clean": "rm -rf node_modules package-lock.json dist cdk.out cdk.context.json pnpm-lock.yaml",
          "clean-start": "pnpm run clean && pnpm run start",
          "build": "tsc",
          "synth": "cdk synth",
          "boot": "cdk bootstrap",
          "destroy": "cdk destroy",
          "deploy": "cdk deploy --all --require-approval never"
     },
     "devDependencies": {
          "@types/jest": "^29.5.11",
          "@types/node": "20.11.6",
          "aws-cdk": "2.124.0",
          "esbuild": "^0.21.5",
          "jest": "^29.7.0",
          "ts-jest": "^29.1.2",
          "ts-node": "^10.9.2",
          "typescript": "~5.3.3"
     },
     "dependencies": {
          "@cny-common/aws.cdk.ts": "*",
          "@cny-helpers/nodejs": "*",
          "aws-cdk-lib": "2.124.0",
          "constructs": "^10.0.0",
          "fs-extra": "^11.2.0"
     }
}